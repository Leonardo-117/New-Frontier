// Inherit the parent event
event_inherited();
//Funções e variaveis
enemy_room_width = function(){
	if x > room_width/2{
		hspeed = -3;
		limit_move_to_sides = false
	} else {
		hspeed = 3
		limit_move_to_sides = false
	}
}
//Modificando posição ao chegar no meio da tela
if y > room_height/3 && limit_move_to_sides = true{
	enemy_room_width();
}